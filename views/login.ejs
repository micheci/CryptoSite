    <div class=".container-fluid">
      <div class="navBar mt-4">
        <div class="logo">
          <a href="/"><h1>MunkyNFTs</h1></a>
        </div>
        <ul class="links">
            <a href=""><li>Team</li></a>
            <a href=""><li>Buy Munky</li></a>
            <a href=/login id="link"><li>Log In</li></a>
            <a href="/signup"><li>Sign In</li></a>
            <li><button class="toggle" onclick="Toggle()">Toggle</button></li>
        </ul>
      </div>
      <div class="row justify-content-center">
    
        <section class="col-6 mt-5">
          <% if (locals.messages.errors) { %> <% messages.errors.forEach( el => { %>
          <div class="alert alert-danger"><%= el.msg %></div>
          <% }) %> <% } %> <% if (locals.messages.info) { %> <%
          messages.info.forEach( el => { %>
          <div class="alert alert-info"><%= el.msg %></div>
          <% }) %> <% } %>
          <form action="/login" method="POST">
            <div class="mb-3">
              <label for="exampleInputEmail1" class="form-label"
                >Email address</label
              >
              <input
                type="email"
                class="form-control"
                id="exampleInputEmail1"
                aria-describedby="emailHelp"
                name="email"
              />
            </div>
            <div class="mb-3">
              <label for="exampleInputPassword1" class="form-label">Password</label>
              <input
                type="password"
                class="form-control"
                id="exampleInputPassword1"
                name="password"
              />
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
          </form>
        </section>
      </div>